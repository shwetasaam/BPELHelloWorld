<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:helloworld="http://helloworld">
  <process name="helloworld:HelloWorld">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="helloworld:HelloWorldService" port="HelloPort"/>
    </provide>
  </process>
</deploy>